{
  "name": "discord-bot-template",
  "displayName": "Discord Bot Template",
  "version": "1.0.0",
  "description": "Apenas um boilerplate de um discord bot com com typescript",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register --transpile-only src/core/index.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --exit-child --transpile-only --ignore-watch node_modules src/core/index.ts",
    "build": "npx tsc --build tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vitu2002/discord-bot-template.git"
  },
  "author": "Vitor Lach <vitu@yomumangas.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vitu2002/discord-bot-template/issues"
  },
  "homepage": "https://github.com/Vitu2002/discord-bot-template#readme",
  "dependencies": {
    "@discordjs/builders": "0.15.0",
    "@discordjs/rest": "0.5.0",
    "@prisma/client": "3.15.1",
    "colors": "1.4.0",
    "discord-api-types": "0.34.0",
    "discord.js": "13.8.0",
    "dotenv": "16.0.1",
    "moment": "2.29.3",
    "prisma": "3.15.1",
    "ts-node": "10.4.0",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@types/node": "17.0.5"
  }
}
